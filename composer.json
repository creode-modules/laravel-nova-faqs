{
    "name": "creode/laravel-nova-faqs",
    "description": "FAQs package for Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "creode/nova-publishable": "^1.1",
        "creode/laravel-repository": "^1.0",
        "laravel/nova": "^4.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "autoload": {
        "psr-4": {
            "Creode\\LaravelNovaFaqs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creode\\LaravelNovaFaqs\\LaravelNovaFaqsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
